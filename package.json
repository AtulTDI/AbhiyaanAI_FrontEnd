{
  "name": "abhiyaan_ai",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start:dev": "cross-env APP_ENV=development expo start --clear",
    "start:prod": "cross-env APP_ENV=production expo start --clear",
    "web:dev": "cross-env APP_ENV=development expo start --web --clear",
    "web:prod": "cross-env APP_ENV=production expo start --web --clear",
    "android:dev": "cross-env APP_ENV=development expo run:android --clear",
    "android:prod": "cross-env APP_ENV=production expo run:android --clear",
    "android:build:dev": "cross-env APP_ENV=development node ./scripts/gradleBuild.js android assembleDebug",
    "android:build:prod": "cross-env APP_ENV=production node ./scripts/gradleBuild.js android assembleRelease",
    "ios:dev": "cross-env APP_ENV=development expo run:ios --clear",
    "ios:prod": "cross-env APP_ENV=production expo run:ios --clear",
    "ios:build:dev": "cross-env APP_ENV=development expo run:ios --configuration Debug --clear",
    "ios:build:prod": "cross-env APP_ENV=production expo run:ios --configuration Release --clear",
    "docker:dev:build": "docker compose -f docker-compose.yml build --no-cache",
    "docker:dev:up": "docker compose -f docker-compose.yml up -d",
    "docker:dev:down": "docker compose -f docker-compose.yml down",
    "docker:dev:logs": "docker compose -f docker-compose.yml logs",
    "docker:dev:restart": "npm run docker:dev:down && npm run docker:dev:up",
    "docker:prod:build": "docker compose -f docker-compose.prod.yml build --no-cache",
    "docker:prod:up": "docker compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker compose -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker compose -f docker-compose.prod.yml logs",
    "docker:prod:restart": "npm run docker:prod:down && npm run docker:prod:up",
    "docker:build:tar:prod": "node ./scripts/createTarDir.js && docker compose -f docker-compose.prod.yml build --no-cache && docker save -o tar/abhiyaan_ai_frontend.tar abhiyaan_ai_frontend:latest"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@microsoft/signalr": "^8.0.7",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/drawer": "^7.5.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "expo": "^53.0.19",
    "expo-av": "~15.1.7",
    "expo-document-picker": "~13.1.6",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "^7.1.7",
    "expo-secure-store": "~14.2.4",
    "expo-sharing": "^13.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "i18next": "^25.5.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.79.6",
    "react-native-contacts": "^8.0.7",
    "react-native-create-thumbnail": "^2.1.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localize": "^3.5.2",
    "react-native-pager-view": "^6.7.1",
    "react-native-paper": "^5.14.5",
    "react-native-paper-dropdown": "^2.3.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-share": "^12.2.0",
    "react-native-tab-view": "^4.1.2",
    "react-native-table-component": "^1.2.2",
    "react-native-walkthrough-tooltip": "^1.6.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@types/react-navigation": "^3.0.8",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
